  "PROBLEMS": {
    "max_score": 40,
    "scoring_method": "snomed_group_weighted",
    "config": {
      "group_mapping_file": "snomed_problem_groups.json",
      "multimorbidity_bonus": {
        "description": "For each active problem group beyond the 3rd, add +1 (capped at +5)",
        "threshold": 3,
        "per_extra_group": 1,
        "cap": 5
      },
      "value_filter": {
        "include_values": [
          "chronic",
          "acute",
          "exist"
        ],
        "exclude_values": [
          "not exist"
        ]
      },
      "score_cap": 40
    }
  },
  "SYMPTOMS": {
    "max_score": 15,
    "scoring_method": "urgency_group_weighted",
    "config": {
      "group_mapping_file": "symptom_urgency_groups.json",
      "severity_multiplier": {
        "severe": 1.5,
        "yes": 1.0,
        "no": 0.0
      },
      "active_symptom_count_bonus": {
        "description": "Bonus for having many active symptoms at once",
        "threshold": 3,
        "bonus": 2
      },
      "score_cap": 15
    }
  },
  "MEDICATIONS": {
    "max_score": 15,
    "keywords": {
      "Medication Count": {
        "type": "range",
        "ranges": [
          {
            "min": 0,
            "max": 4,
            "score": 0,
            "label": "Low"
          },
          {
            "min": 5,
            "max": 9,
            "score": 1,
            "label": "Moderate"
          },
          {
            "min": 10,
            "max": 14,
            "score": 2,
            "label": "High"
          },
          {
            "min": 15,
            "max": 999,
            "score": 4,
            "label": "Very high (polypharmacy)"
          }
        ]
      },
      "New Medications Count": {
        "type": "range",
        "ranges": [
          {
            "min": 0,
            "max": 1,
            "score": 0,
            "label": "Minimal"
          },
          {
            "min": 2,
            "max": 2,
